//Display Symbol Î©
void setup() {
  volatile char *dirf, *dirk;
  dirf = 0x30;
  *dirf = 0xFF;
  dirk = 0x107;
  *dirk = 0xFF;
}
void loop() {
  volatile char *outf, *outk;
  outf = 0x31;
  outk = 0x108;
  volatile long i;
  while (1) {
    *outk = 0xDF;
    *outf = 0x3C;
    for (i = 0; i < 5000; i++);
    *outk = 0xEF;
    *outf = 0x42;
    for (i = 0; i < 5000; i++);
    *outk = 0xF7;
    *outf = 0x42;
    for (i = 0; i < 5000; i++);
    *outk = 0xFB;
    *outf = 0x42;
    for (i = 0; i < 5000; i++);
    *outk = 0xFD;
    *outf = 0xE7;
    for (i = 0; i < 5000; i++);

  }
}

